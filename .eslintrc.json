{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "overrides": [
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "no-console": 1,
      "no-empty": 0,
      "no-irregular-whitespace": 1,
      "no-var": 1,
      "no-unused-vars": 0,
      "camelcase": 0,
      "eqeqeq": 0,
      "no-label-var": 2,
      "semi":1,
      "@typescript-eslint/no-explicit-any": 0
    }
}
